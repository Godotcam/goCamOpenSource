{
	"compilerOptions": {
		"ignoreDeprecations": "5.0",
		"charset": "utf-8",
		"target": "es3",
		"removeComments": false,
		"preserveConstEnums": false,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"rootDir": "./app",
		"sourceMap": true,
		"listFiles": false,
		"listEmittedFiles": false,
		"strict": true,
		"pretty": true,
		"declaration": false,
		"strictNullChecks": false,
		"allowSyntheticDefaultImports": true
	}
}
